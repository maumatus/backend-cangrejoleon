<!DOCTYPE html>
<html>

  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>

  <body>
    <h1><%= title %></h1>
    <h2><% data.length %> </h2>
    <table>
      <tr>
        <th>Titulo</th>
        <th>Isbn</th>
        <th>Paginas</th>
        <th>Fecha Publicación</th>
        <th>Descripción</th>
        <th>Portada</th>
        <th>Status</th>
        <th>Autores</th>
        <th>Categorías</th>
      </tr>
      <% data.forEach(function(el) { %>
        <tr>
          <td>
            <%= el.title %>
          </td>
          <td>
            <%= el.isbn %>
          </td>
          <td>
            <%= el.pageCount %>
          </td>
          <td>
            <%= el.publishedDate %>
          </td>
          <td>
            <%= el.shortDescription %>
          </td>
          <td>
            <img src= <%= el.thumbnailUrl %> alt="img" width="100">
          </td>
          <td>
            <%= el.status %>
          </td>
          <td>
            <%= el.authors %>
          </td>
          <td>
            <%= el.categories %>
          </td>
        </tr>
        <% }) %>
    </table>
  </body>
</html>
